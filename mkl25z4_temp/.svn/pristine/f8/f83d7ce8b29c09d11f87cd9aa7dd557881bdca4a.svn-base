	.syntax unified
	.thumb

	.global sum_asm

/*
 *==============================================================================
 * sum_asm
 *==============================================================================
 */

	.section .text.sum_asm, "ax", %progbits
	.type sum_asm, %function
	.thumb_func
sum_asm:

	adds r0, r1
	ldr r1, =0
	adcs r1, r1
	bx lr

	.size sum_asm, . - sum_asm

/*
 *==============================================================================
 * EOF
 *==============================================================================
 */
	.end
